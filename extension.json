{
	"name": "ConfIDentSkin",
	"version": "1.0.6",
	"author": [
		"gesinn-it-wam (gesinn.it GmbH & Co. KG)"
	],
	"url": "https://github.com/gesinn-it-pub/ConfIDentSkin",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35"
	},
	"AutoloadNamespaces": {
